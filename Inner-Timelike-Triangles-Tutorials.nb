Cell[BoxData[
 RowBox[{
  RowBox[{"\[Theta]2DL", "[", 
   RowBox[{"l12Sq_", ",", "l13Sq_", ",", "l23Sq_"}], "]"}], ":=", 
  RowBox[{"If", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"V2Sq", "[", 
      RowBox[{"l12Sq", ",", "l13Sq", ",", "l23Sq"}], "]"}], "<", "0"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"If", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"l12Sq", ">", "0"}], "&&", 
       RowBox[{"l13Sq", ">", "0"}]}], " ", 
      RowBox[{"(*", "SSX", "*)"}], ",", "\[IndentingNewLine]", 
      RowBox[{"If", "[", " ", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"l23Sq", ">", "0"}], " ", "&&", " ", 
         RowBox[{
          SqrtBox["l23Sq"], " ", "\[GreaterEqual]", 
          RowBox[{
           SqrtBox["l12Sq"], "+", 
           SqrtBox["l13Sq"]}]}]}], " ", ",", "\[IndentingNewLine]", 
        RowBox[{"(*", 
         StyleBox[
          RowBox[{"S", 
           StyleBox["ss",
            FontVariations->{"Underline"->True}]}]], "*)"}], 
        RowBox[{
         RowBox[{
          RowBox[{"-", "\[ImaginaryI]"}], " ", "\[Pi]"}], "-", 
         RowBox[{"ArcCosh", "[", 
          RowBox[{"-", 
           FractionBox[
            RowBox[{"l12Sq", "+", "l13Sq", "-", "l23Sq"}], 
            RowBox[{"2", " ", 
             SqrtBox[
              RowBox[{"l12Sq", " ", "l13Sq"}]]}]]}], "]"}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", 
         StyleBox["Ssx",
          FontVariations->{"Underline"->True}], "*)"}], 
        RowBox[{"ArcCosh", "[", 
         FractionBox[
          RowBox[{"l12Sq", "+", "l13Sq", "-", "l23Sq"}], 
          RowBox[{"2", " ", 
           SqrtBox[
            RowBox[{"l12Sq", " ", "l13Sq"}]]}]], "]"}]}], 
       "\[IndentingNewLine]", "]"}], ",", "\[IndentingNewLine]", 
      RowBox[{"If", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"l12Sq", ">", "0"}], "&&", 
           RowBox[{"l13Sq", "<", "0"}]}], " ", ")"}], "||", " ", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"l12Sq", "<", "0"}], "&&", 
           RowBox[{"l13Sq", ">", "0"}]}], " ", ")"}]}], " ", 
        RowBox[{"(*", "STX", "*)"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"ArcSinh", "[", 
          FractionBox[
           RowBox[{"l12Sq", "+", "l13Sq", "-", "l23Sq"}], 
